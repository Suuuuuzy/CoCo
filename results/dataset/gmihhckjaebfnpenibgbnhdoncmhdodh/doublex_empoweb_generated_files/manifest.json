{
"update_url": "https://clients2.google.com/service/update2/crx",

   "author": "JB Design",
   "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
   "content_scripts": [ {
      "css": [ "css/content.css" ],
      "js": [ "js/injector.js" ],
      "matches": [ "*://www.kickstarter.com/*" ]
   } ],
   "description": "Show prices in your currency on kickstarter",
   "icons": {
      "128": "img/icon128.png",
      "128": "img/icon48.png",
      "128": "img/icon32.png"
   },
   "manifest_version": 2,
   "name": "ksCurrency",
   "page_action": {
      "default_icon": "img/icon128.png",
      "default_popup": "popup.html",
      "default_title": "Options"
   },
   "permissions": [ "storage", "http://www.kickstarter.com/", "activeTab", "declarativeContent", "tabs", "webNavigation" ],
   "short_name": "ksCurrency",
   "version": "0.0.9",
   "web_accessible_resources": [ "img/*.png", "js/jquery-2.1.4.min.js", "js/content.js", "css/content.css","js/jquery-ajax.js" ]
}
