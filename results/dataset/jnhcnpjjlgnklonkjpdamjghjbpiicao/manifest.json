{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "2.1.6",
  "manifest_version": 2,
  "name": "Trainman",
  "description": "Indian railway PNR Status Prediction, Seat Availability & Train Status",
  "author": "@Trainman",
  "key": "",
  "content_scripts": [
    {
      "matches": [
        "*://*.indianrail.gov.in/*"
      ],
      "js": [
        "jquery-1.8.2.min.js",
        "run.js",
        "inject.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "webRequest",
    "*://*.indianrail.gov.in/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.flightman.in/*",
      "*://*.trainman.in/*",
      "*://*.localhost2.ab/*"
    ]
  },
  "web_accessible_resources": [
    "injected.js"
  ],
  "icons": {
    "16": "images/logo_16.png",
    "32": "images/logo_32.png",
    "48": "images/logo_48.png",
    "128": "images/logo_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
